{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "rex:wingey",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "despawn": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:experience_reward": {
                "on_death": "query.last_hit_by_player ? 5 + (query.equipment_count * Math.Random(1,3)) : 0"
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0
            },
            "minecraft:timer": {
                "looping": false,
                "time": 10,
                "time_down_event": {
                    "event": "despawn"
                }
            },
            "minecraft:fire_immune": {},
            "minecraft:nameable": {},
            "minecraft:health": {
                "value": 20,
                "max": 20
            },
            "minecraft:collision_box": {
                "width": 0.5,
                "height": 0.7
            },
            "minecraft:behavior.follow_owner": {
                "priority": 1,
                "start_distance": 10,
                "stop_distance": 2
            },
            "minecraft:movement": {
                "value": 0.06
            },
            "minecraft:movement.fly": {
                "max_turn": 1
            },
            "minecraft:scale": {
                "value": 1.5
            },
            "minecraft:navigation.float": {
                "can_path_over_water": true
            },
            "minecraft:behavior.float_wander": {
                "priority": 2,
                "must_reach": true,
                "random_reselect": true,
                "float_duration": [
                    2,
                    3
                ],
                "float_wander_has_move_control": false
            },
            "minecraft:can_fly": {},
            "minecraft:shooter": {
                "def": "rex:eyesball"
            },
            "minecraft:type_family": {
                "family": [
                    "soul",
                    "mob"
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "fall",
                    "deals_damage": "no"
                }
            },
            "minecraft:jump.static": {},
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:behavior.float": {
                "priority": 0
            },
            "minecraft:behavior.ranged_attack": {
                "priority": 1,
                "attack_radius": 28,
                "charge_shoot_trigger": 1,
                "charge_charged_trigger": 1
            },
            "minecraft:follow_range": {
                "value": 64,
                "max": 64
            },
            "minecraft:behavior.hurt_by_target": {
                "priority": 3,
                "entity_types": [
                {
                    "filters": {
                        "all_of": [
                        {
                            "test": "has_tag",
                            "subject": "other",
                            "operator": "!=",
                            "value": "soul"
                        }]
                    },
                    "max_dist": 64
                }]
            },
            "minecraft:behavior.nearest_attackable_target": {
                "priority": 2,
                "entity_types": [
                {
                    "filters": {
                        "test": "is_family",
                        "subject": "other",
                        "value": "monster"
                    },
                    "max_dist": 64
                },
                {
                    "filters": {
                        "test": "is_family",
                        "subject": "other",
                        "value": "inanimate"
                    },
                    "max_dist": 64
                },
                {
                    "filters": {
                        "test": "is_family",
                        "subject": "other",
                        "value": "irongolem"
                    },
                    "max_dist": 64
                },
                {
                    "filters": {
                        "test": "is_family",
                        "subject": "other",
                        "value": "boss"
                    },
                    "max_dist": 64
                },
                {
                    "filters": {
                        "all_of": [
                        {
                            "test": "is_family",
                            "subject": "other",
                            "operator": "!=",
                            "value": "soul"
                        },
                        {
                            "test": "is_family",
                            "subject": "other",
                            "operator": "!=",
                            "value": "proj"
                        },
                        {
                            "test": "is_family",
                            "subject": "other",
                            "operator": "!=",
                            "value": "damage"
                        },
                        {
                            "test": "is_family",
                            "subject": "other",
                            "value": "player"
                        },
                        {
                            "test": "has_tag",
                            "subject": "other",
                            "operator": "!=",
                            "value": "soul"
                        }]
                    },
                    "max_dist": 64
                }],
                "must_see": true
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:cannot_be_attacked": {}
        },
        "events": {
            "minecraft:entity_spawned": {
                "queue_command": {
                    "command": "particle rex:soulspawn"
                }
            },
            "despawn": {
                "add": {
                    "component_groups": [
                        "despawn"
                    ]
                },
                "queue_command": {
                    "command": [
                        "particle rex:soulimpactz ~~~"
                    ]
                }
            }
        }
    }
}