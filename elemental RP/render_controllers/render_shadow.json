{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.render_shadow": {
			"arrays": {
				"textures": {
					"array.frames": [
						"texture.frame_0",
						"texture.frame_1",
						"texture.frame_2",
						"texture.frame_3",
						"texture.frame_4"
					]
				}
			},
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "variable.is_enchanted ? material.enchanted : material.default"
				}
			],
			"textures": [
				"temp.life_time = query.life_time * 8.0; return array.frames[temp.life_time];",
				"texture.enchanted"
			]
		}
	}
}